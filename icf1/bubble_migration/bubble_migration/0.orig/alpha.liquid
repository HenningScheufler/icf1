FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alpha.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    tank
    {
        type            constantAlphaContactAngle;
        theta0		0;
        limit		zeroGradient;
        value		uniform 0;
    }

    symm_side
    {
        type            symmetry;
    }
}

