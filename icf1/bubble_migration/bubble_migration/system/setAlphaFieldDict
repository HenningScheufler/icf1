FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system/fluid";
    object      setAlphaFieldDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

field "alpha.liquid";

type ellipsoid;
origin (0 0.0185 0.10);
semiAxis (0.01 0.01 0.03);
invert true;


